<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Payments.Mvc</title>
    <base href="~/" />
    <environment names="Development">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.css" />
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.css" />
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.1/css/bootstrap.min.css" integrity="sha384-Gn5384xqQ1aoWXA+058RXPxPg6fy4IWvTNh0E263XmFcJlSAwiGgFAW/dAiS6JXm" crossorigin="anonymous">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.css" />
        <link rel="stylesheet" href="~/css/report.css" asp-append-version="true" />
    </environment>
    @RenderSection("AdditionalStyles", required: false)
</head>
<body>
  <div class="logo m-b t-center">
    <a href="/">
        @Html.InlineSvg("/wwwroot/media/logo.svg")
    </a>
  </div>
    <div class="container">
        @RenderBody()
    </div>
      @Html.Partial("_Footer")

    @RenderSection("scripts", required: false)
</body>
</html>
