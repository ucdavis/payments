@model Payments.Mvc.Models.WebHookModels.CreateWebHookViewModel
@{
    ViewData["Title"] = "Create WebHook";
}

<form asp-action="CreateWebHook" method="post">
    <div class="card-style">
        <div class="card-header-primary card-bot-border">
            <div class="card-head d-flex justify-content-between align-items-start">
                <h2>@ViewBag.Title</h2>
                <a asp-action="WebHooks">Back</a>
            </div>
        </div>
        
        <partial name="_WebHookForm" />
    </div>
</form>

<div>
    <a asp-action="WebHooks"><i class="fas fa-chevron-left mr-3"></i> Back to WebHooks</a>
</div>